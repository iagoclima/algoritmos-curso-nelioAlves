Algoritmo "senha_fixa"

Var

   senhaValida, senha : inteiro


Inicio

   senhaValida <- 2002
   escreva("Digite a senha: ")
   leia(senha)
   
   enquanto senha <> senhaValida faca
      escreva("Senha Invalida! Tente novamente: ")
      leia(senha)
   fimenquanto
   
   escreva("Acesso permitido!")


Fimalgoritmo
